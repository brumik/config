Host *
  AddKeysToAgent yes
  UseKeychain yes
  # IdentityFile ~/.ssh/id_ed25519

# Default GitHub Account (for all other repos)
Host github.com
  AddKeysToAgent yes
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_ed25519 

# Account for github.com/modmed organization
Host mmgithub
  AddKeysToAgent yes
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_ed25519_mm 

Host stash.modernizingmedicine.com
  Port 7999
  IdentityFile ~/.ssh/id_ed25519
